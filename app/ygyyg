function myFunction() {     
  
    // Gets the various data from the HTML formvar status = document.getElementById("submit");
    var status = document.getElementById("submit");
    var status2 = document.getElementById("submit2");
  
    // Push both the captured form data up to the 5th Years branch of the database with labels (purple)
    feebackBranch.push({ Yes: status.value, No: status2.value});

    // Display a message to let user know the upload has been successful
    alert("Thank you for your feedback, the following has been added to the database - " + status.value || + status2.value)

} 





